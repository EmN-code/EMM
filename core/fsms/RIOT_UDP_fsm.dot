// FSM
digraph {
	INIT [label=INIT]
	INIT -> TIMER_SETUP [label="SERVER_IP_VALID && SOCKET_CREATED"]
	INIT -> ERROR [label="!SERVER_IP_VALID"]
	INIT -> ERROR [label="!SOCKET_CREATED"]
	TIMER_SETUP [label=TIMER_SETUP]
	TIMER_SETUP -> SEND_STATE [label=TIMER_INITIALIZED]
	SEND_STATE [label=SEND_STATE]
	SEND_STATE -> SEND_STATE [label=MSG_RECEIVED]
	ERROR [label=ERROR]
	ERROR -> INIT [label=RETRY_CONNECTION]
}
